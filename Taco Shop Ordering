#include <iostream>
#include <iomanip>
//nclude "Tipodetacos.h"

using namespace std;

int main()
{
  const int numtipodetacos = 5;
  Tipodetacos taco[numtipodetacos] = {
    Tipodetacos ("Asada", 2.00),
    Tipodetacos ("Al Pastor", 2.00),
    Tipodetacos ("Barbacoa", 2.50),
    Tipodetacos ("Lengua", 2.50),
    Tipodetacos ("Tripas", 2.50)
  };

  cout << setw (12) << "Tipo De Taco" << setw (12) << "Precio\n";
  cout << "__________________________________" << endl;

  for (int i = 0; i < numtipodetacos; i++)
    {
      cout << setw (12) << taco[i].getDescription ();
      cout << setw (12) << taco[i].getCost();
      <<endl;
    }
  return 0;
}
